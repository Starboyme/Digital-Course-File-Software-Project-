                <a href="/displayfiles?courseid=<%=courseid %>&username=<%=username%>&type=1&filetype=q" id="questionpaper" class="btn sidebarr pl-1 pr-1">Question Papers</a>
                <a href="/displayfiles?courseid=<%=courseid %>&username=<%=username%>&type=3&filetype=c" id="coursematerial" class="btn sidebarr pl-1 pr-1">Class Material</a>
                <a href="/displayfiles?courseid=<%=courseid %>&username=<%=username%>&type=4&filetype=g" id="grades" class="btn sidebarr pl-1 pr-1">Grades</a>
                <a href="/displayfiles?courseid=<%=courseid %>&username=<%=username%>&type=5&filetype=s" id="searchfiles" class="btn sidebarr pl-1 pr-1">Search Files</a>
                  
