        <div class="col-10">
          <div class="d-flex bd-highlight" id="ProfileBox">
            <div  class="p-2 flex-grow-1 bd-highlight">
              <% if(type==1){ %>
                <h1 class="text-center display-4 my-4" id="inputquestionpaper">Question paper</h1>
                <form action="/upload?courseid=<%=courseid %>&username=<%=username%>&filetype=q&type=1" method="POST" enctype="multipart/form-data">
                  <div class="custom-file mb-3">
                    <input type="file" name="file" id="file" class="custom-file-input" onchange="filename()">
                    <label for="file"  id="fileInput" class="custom-file-label" >Choose File</label>
                  </div>
                  <input type="submit" value="Submit" id="filesubmit" class="btn btn-primary btn-block">
                </form>

                <% if(files){ %>
                  <% files.forEach(function(file) { %>
                    <div class="container">
                    <div class="card card-body mb-3">
                          <a href="file/<%= file.filename %>" target="_blank" >Read more</a>
                          <span  id="showquestionpaper"><%= file.filename %></span>
                            <!-- <form method="POST" action="/files/<%= file._id %>?operation=Delete&courseid=<%=courseid %>&username=<%=username%>&type=1">
                              <button id="deletefile" class="btn btn-danger btn-block mt-4">Delete</button>
                            </form> -->
                            <a id='deletefile' href="/files/<%= file._id %>?operation=Delete&courseid=<%=courseid %>&username=<%=username%>&type=1&filetype=q">Delete</a>
                    </div>
                    </div>
                    <% }) %>
                      <% } else { %>
                        <p>No files to show</p>
                        <% } %>


                <% } else if(type==2){ %>
                <div class="container">
                <h1 class="text-center display-4 my-4" id="inputprojectmaterial">Project Materials</h1>
                <form action="/upload?courseid=<%=courseid %>&username=<%=username%>&filetype=p&type=2" method="POST" enctype="multipart/form-data">
                  <div class="custom-file mb-3">
                    <input type="file"  name="file" id="file" class="custom-file-input" onchange="filename()">
                    <label for="file" id="fileInput" class="custom-file-label" >Choose File</label>
                  </div>
                  <input type="submit" value="Submit" id="filesubmit" class="btn btn-primary btn-block">
                </form>
                </div>

                <% if(files){ %>
                  <% files.forEach(function(file) { %>
                    <div class="container">
                    <div class="card card-body mb-3">
                          <a href="file/<%= file.filename %>" target="_blank">Read more</a>
                           <span id="showprojectmaterial"><%= file.filename %></span> 
                            <!-- <form method="POST" action="/files/<%= file._id %>?operation=Delete&courseid=<%=courseid %>&username=<%=username%>&type=2">
                              <button id="deletefile" class="btn btn-danger btn-block mt-4">Delete</button>
                            </form> -->
                            <a id="deletefile" href="/files/<%= file._id %>?operation=Delete&courseid=<%=courseid %>&username=<%=username%>&type=2&filetype=p">Delete</a>
                    </div>
                    </div>
                    <% }) %>
                      <% } else { %>
                        <p>No files to show</p>
                        <% } %>

                <% } else if(type==3){ %>
                <div class="container">
                <h1 class="text-center display-4 my-4" id="inputclassmaterial">Class Materials</h1>
                <form action="/upload?courseid=<%=courseid %>&username=<%=username%>&filetype=c&type=3" method="POST" enctype="multipart/form-data">
                  <div class="custom-file mb-3">
                    <input type="file"  name="file" id="file" class="custom-file-input" onchange="filename()">
                    <label for="file" id="fileInput" class="custom-file-label" >Choose File</label>
                  </div>
                  <input type="submit" value="Submit" id="filesubmit" class="btn btn-primary btn-block">
                </form>
                </div>
                <% if(files){ %>
                  <% files.forEach(function(file) { %>
                    <div class="container">
                    <div class="card card-body mb-3">
                          <a href="file/<%= file.filename %>" target="_blank">Read more</a>
                          <span id="showclassmaterial"><%= file.filename %></span>
                            <!-- <form method="POST" action="/files/<%= file._id %>?operation=Delete&courseid=<%=courseid %>&username=<%=username%>&type=3">
                              <button id="deletefile" class="btn btn-danger btn-block mt-4">Delete</button>
                            </form> -->
                            <a id="deletefile" href="/files/<%= file._id %>?operation=Delete&courseid=<%=courseid %>&username=<%=username%>&type=3&filetype=c">Delete</a>
                    </div>
                    </div>
                    <% }) %>
                      <% } else { %>
                        <p>No files to show</p>
                        <% } %>


                <% } else if(type==4){ %>
                <div class="container">
                <h1 class="text-center display-4 my-4" id="inputgrades">Grades</h1>
                <form action="/upload?courseid=<%=courseid %>&username=<%=username%>&filetype=g&type=4" method="POST" enctype="multipart/form-data">
                  <div class="custom-file mb-3">
                    <input type="file" name="file" id="file" class="custom-file-input" onchange="filename()"><br>
                    <label for="file"  id="fileInput" class="custom-file-label" >Choose File</label><br>
                  <input type="submit" value="Submit" id="filesubmit" class="btn btn-primary btn-block">
                </form>
                </div>
                <% if(files){ %>
                  <% files.forEach(function(file) { %>
                    <div class="container">
                      <div class="card card-body mb-3">
                            <a href="file/<%= file.filename %>" target="_blank">Read more</a>
                            <span id="showclassmaterial"><%= file.filename %></span>
                              <!-- <form method="POST" action="/files/<%= file._id %>?operation=Delete&courseid=<%=courseid %>&username=<%=username%>&type=3">
                                <button id="deletefile" class="btn btn-danger btn-block mt-4">Delete</button>
                              </form> -->
                              <a id="deletefile" href="/files/<%= file._id %>?operation=Delete&courseid=<%=courseid %>&username=<%=username%>&type=4&filetype=g">Delete</a>
                      </div>
                      </div>
                    <% }) %>
                      <% } else { %>
                        <p>No files to show</p>
                        <% } %>


                <% } else if(type==5){ %>

                  <div class="container">
                    <h1 class="text-center display-4 my-4" id="inputgrades">Search Files</h1>
                    
                    <form action="/search?courseid=<%=courseid %>&username=<%=username%>&type=5" method="POST">
                      <div class="custom-file mb-3">
                        <input type="text" id="searchName" class="form-control" class="filtrname" name="fname" placeholder="filename">
                        <!-- <input type="text" name="p1"> -->
                        <!-- <label for="file" class="custom-file-label" onchange="filename()">Choose File</label> -->
                        <script>
                        function filename()
                        {
                            var x=document.getElementById("file").value.split(/(\\|\/)/g).pop();
                            document.getElementById("fileInput").innerHTML=x;
                            return x;
                        }
                        </script>
                      </div>
                      <input type="submit" value="Submit" id="filesubmit" class="btn btn-primary btn-block">
                    </form>
                    </div>

                    <% if(files){ %>
                      <% files.forEach(function(file) { %>
                        <div class="container">
                          <div class="card card-body mb-3">
                                <a href="file/<%= file.filename %>" target="_blank">Read more</a>
                                <span id="showclassmaterial"><%= file.filename %></span>
                                  <!-- <form method="POST" action="/files/<%= file._id %>?operation=Delete&courseid=<%=courseid %>&username=<%=username%>&type=3">
                                    <button id="deletefile" class="btn btn-danger btn-block mt-4">Delete</button>
                                  </form> -->
                                  <a id="deletefile" href="/files/<%= file._id %>?operation=Delete&courseid=<%=courseid %>&username=<%=username%>&type=5">Delete</a>

                          </div>
                          </div>
                        <% }) %>
                          <% } else { %>
                            <p>No files to show</p>
                            <% } %>

                  <% } else if(type==6){ %>
                    <h1 class="display-3 text-center mt-2">Feedbacks</h1>
                    <div class="container-fluid w-80 m-auto">
                        <table class="table text-white mt-2">
                            <thead class="thead-dark">
                              <th scope="col">Date</th>
                              <th scope="col">Time</th>
                              <th scope="col">Student_ID</th>
                              <th scope="col">Feedback</th>
                              <th scope="col">Course_ID</th>
                              <th scope="col">Faculty_ID</th>
                            </thead>
                            <% if(results){ %>
                              <% results.forEach(function(result) { %>
                                  <tbody style="color: black;">
                                      <td><%= result.curdate %></td>
                                      <td><%= result.curtime %></td>
                                      <td><%= result.student_id %></td>
                                      <td><%= result.message %></td>
                                      <td><%= result.course_id %></td>
                                      <td><%= result.faculty_id %></td>
                                  </tbody>
                                  <% }) %>
                                <% } else { %>
                                  <tbody style="color: black;">
                                    <td colspan="6" class="text-center" style="color: black;font-weight: bolder;">No feedbacks available</td>
                                  </tbody>
                                  <% } %>
                        </table>
                    </div>
                  <% } else { %>

                    <% } %>
              </div>   
            </div>
            <!-- <div class="p-2 bd-highlight">Flex item</div>
            <div class="p-2 bd-highlight">Third flex item</div> -->
        </div>
        </div>
      </div>
    </div>

    <script>
      $(function(){
        $('#searchName').autocomplete({
  
          source:function(req,res){
            // console.log(req);
            $.ajax({
              url:"autocomplete?courseid=<%=courseid %>&username=<%=username %>",
              dataType:"jsonp",
              type:"GET",
              data: req,
              success: function(data){
                console.log(data);
                res(data)
              },
              error: function(err){
                console.log(err.status);
              }
            });
          },
  
          minLength:1,
          select: function(event,ui){
            if(ui.item){
              $('#searchName').text(ui.item.label);
            }
          }
  
  
        });
      });
  
    </script>

    
