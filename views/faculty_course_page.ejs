                <!-- <form action="/type?courseid=<%=courseid %>&username=<%=username%>">
                  <input type="radio" class="btn-check" name="type" id="btnradio" autocomplete="off" onchange="theForm.submit()">
                  <label class="btn btn-outline" for="btnradio1">Project Material</label>
                
                  <input type="radio" class="btn-check" name="type" id="btnradio" autocomplete="off" onchange="theForm.submit()">
                  <label class="btn btn-outline" for="btnradio2">Class Material</label>
                
                  <input type="radio" class="btn-check" name="type" id="btnradio" autocomplete="off" onchange="theForm.submit()">
